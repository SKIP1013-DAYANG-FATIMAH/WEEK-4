package meow;

public class Boo3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean b1 = true;
		boolean b2 = false;
		boolean b3 = 2 < 3; //less than operator
		boolean b4 = 2 > 3;
		
		System.out.println(b1);
		System.out.println(b2);
		System.out.println(b3);
		System.out.println(b4);
		
		
		
	}

}
